{
  "name": "notebook-navigator",
  "version": "1.3.3",
  "description": "Replace the default file explorer with a clean two-pane interface featuring folder tree, tag browsing, file previews, keyboard navigation, drag-and-drop, pinned notes, and customizable display options.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "Johan Sanneblad",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.19.120",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.8",
    "knip": "^5.62.0",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "date-fns": "^4.1.0",
    "emojilib": "^4.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
